{
  "$schema": "https://schema.dash.org/dpp-0-4-0/meta/dash-schema",
  "$id": "https://schema.dash.org/dpp-0-4-0/base/document",
  "type": "object",
  "properties": {
    "$type": {
      "type": "string"
    },
    "$rev": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 1
    },
    "$contractId": {
      "type": "string",
      "minLength": 42,
      "maxLength": 44,
      "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$"
    },
    "$userId": {
      "type": "string",
      "minLength": 42,
      "maxLength": 44,
      "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$"
    },
    "$entropy": {
      "type": "string",
      "minLength": 34,
      "maxLength": 34
    }
  },
  "required": [
    "$type",
    "$rev",
    "$contractId",
    "$userId",
    "$entropy"
  ],
  "additionalProperties": false
}
