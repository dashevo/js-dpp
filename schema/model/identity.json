{
  "$id": "https://schema.dash.org/dpp-0-4-0/model/identity",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 42,
      "maxLength": 44,
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
    },
    "type": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 0,
      "maximum": 65535
    },
    "publicKeys": {
      "type": "array",
      "items": {
        "$ref": "https://schema.dash.org/dpp-0-4-0/base/public-key"
      },
      "minItems": 1,
      "maxItems": 100
    }
  },
  "required": [
    "id",
    "type",
    "publicKeys"
  ]
}
