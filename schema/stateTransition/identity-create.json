{
  "$id": "https://schema.dash.org/dpp-0-4-0/state-transition/identity-create",
  "properties": {
    "identityCreateStateTransitionVersion": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 0
    },
    "lockedOutPoint": {
      "type": "string",
      "minLength": 48,
      "maxLength": 48,
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
    },
    "identityType": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 0,
      "maximum": 65535
    },
    "publicKeys": {
      "type": "array",
      "items": {
        "$ref": "https://schema.dash.org/dpp-0-4-0/base/public-key"
      },
      "minItems": 1,
      "maxItems": 10
    },
    "ownershipProofSignature": {
      "type": "string",
      "minLength": 86,
      "maxLength": 87,
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
    }
  },
  "required": [
    "identityCreateStateTransitionVersion",
    "lockedOutPoint",
    "identityType",
    "publicKeys",
    "ownershipProofSignature"
  ]
}
