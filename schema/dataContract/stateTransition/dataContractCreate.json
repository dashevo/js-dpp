{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "protocolVersion": {
      "type": "integer",
      "minimum": 0,
      "maximum": 0,
      "$comment": "Maximum is the latest Data Contract Create Transition protocol version"
    },
    "dataContract": {
      "type": "object"
    },
    "entropy": {
      "type": "string",
      "minLength": 26,
      "maxLength": 35,
      "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$"
    }
  },
  "required": [
    "protocolVersion",
    "dataContract",
    "entropy"
  ]
}
