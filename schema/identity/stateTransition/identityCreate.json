{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "protocolVersion": {
      "type": "integer",
      "minimum": 0,
      "maximum": 0,
      "$comment": "Maximum is the latest Identity Create Transition protocol version"
    },
    "lockedOutPoint": {
      "type": "string",
      "minLength": 48,
      "maxLength": 48,
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
    },
    "publicKeys": {
      "type": "array",
      "minItems": 1,
      "maxItems": 10
    }
  },
  "required": [
    "protocolVersion",
    "lockedOutPoint",
    "publicKeys"
  ]
}
