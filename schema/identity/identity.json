{
  "$id": "https://schema.dash.org/dpp-0-4-0/identity/identity",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 42,
      "maxLength": 44,
      "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$"
    },
    "publicKeys": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100
    },
    "balance": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": [
    "id",
    "publicKeys",
    "balance"
  ]
}
