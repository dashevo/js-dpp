{
  "$id": "https://schema.dash.org/dpp-0-4-0/idenitity/state-transitions/identity-create",
  "properties": {
    "lockedOutPoint": {
      "type": "string",
      "minLength": 48,
      "maxLength": 48,
      "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$"
    },
    "type": {
      "type": "number",
      "multipleOf": 1.0,
      "minimum": 0,
      "maximum": 65535
    },
    "publicKeys": {
      "type": "array",
      "items": {
        "$ref": "https://schema.dash.org/dpp-0-4-0/identity/public-key"
      },
      "minItems": 1,
      "maxItems": 10
    }
  },
  "required": [
    "lockedOutPoint",
    "identityType",
    "publicKeys"
  ]
}
